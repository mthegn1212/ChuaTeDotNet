										<h3><a href="@Url.Action("Details", "Properties", new { id = 1 })">Fatima Subdivision</a></h3>
									</div>
									<div class="pl-md-4">
										<h4 class="price">$120,000</h4>
									</div>
								</div>
								<p class="h-info"><span class="location">New York</span> <span class="details">&mdash; 3bds, 2bath</span></p>
